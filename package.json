{
	"name": "egg-plugin-xml",
	"description": "An eggjs plugin for parsing xml",
	"version": "1.1.0",
	"packageManager": "pnpm@7.14.2",
	"eggPlugin": {
		"name": "xmlParser"
	},
	"files": [
		"app",
		"config",
		"app.js"
	],
	"scripts": {
		"test": "pnpm run lint -- --fix && egg-bin pkgfiles && pnpm run test-local",
		"test-local": "egg-bin test",
		"cov": "egg-bin cov",
		"lint": "eslint .",
		"ci": "egg-bin pkgfiles --check && pnpm run lint && pnpm run cov",
		"pkgfiles": "egg-bin pkgfiles",
		"pub": "tscjs scripts/publish",
		"unpub": "tscjs scripts/unpublish",
		"sync": "tscjs scripts/sync",
		"dist": "run-s eslint prettier pub",
		"autod": "autod",
		"eslint": "eslint --fix --ext .ts,.js ./",
		"prettier": "prettier --write \"**/*.{js,ts,json,md}\""
	},
	"dependencies": {
		"js-cool": "^2.5.0",
		"raw-body": "^2.5.1",
		"xml2js": "^0.4.23"
	},
	"devDependencies": {
		"@eslint-sets/eslint-config-simple": "^3.0.5",
		"autod": "^3.1.2",
		"autod-egg": "^1.1.0",
		"egg": "^3.5.0",
		"egg-bin": "^6.0.0",
		"egg-ci": "^2.2.0",
		"eslint": "^8.27.0",
		"eslint-config-egg": "^12.1.0",
		"eslint-plugin-jsdoc": "^39.6.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"prettier-config-common": "^1.3.0",
		"tsnd": "^1.1.0",
		"webstorm-disable-index": "^1.2.0"
	},
	"peerDependencies": {
		"egg": ">= 2",
		"egg-bin": ">= 4",
		"js-cool": ">= 2",
		"raw-body": "^2.0.0",
		"xml2js": ">= 0.4"
	},
	"keywords": [
		"eggjs",
		"eggPlugin",
		"egg-plugin-xml",
		"egg-plugin",
		"xml",
		"egg-xml"
	],
	"engines": {
		"node": ">=12.0.0"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"@babel/core",
				"@types/node",
				"webpack",
				"typescript",
				"mocha"
			],
			"allowedVersions": {
				"eslint": ">= 8.0.0",
				"fsevents": ">= 2.0.0"
			}
		}
	},
	"resolutions": {
		"fsevents": ">= 2.0.0"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	},
	"ci": {
		"version": "12, 14, 16, 18"
	},
	"license": "MIT",
	"author": "saqqdy <https://github.com/saqqdy>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/egg-plugin-xml.git"
	},
	"bugs": {
		"url": "https://github.com/saqqdy/egg-plugin-xml/issues"
	},
	"homepage": "https://github.com/saqqdy/egg-plugin-xml#readme"
}
